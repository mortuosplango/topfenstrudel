{
  "manifest_version": 2,
  "name": "Topfenstrudel",
  "version": "1.1.2",

  "description": "Adds a strudel repl on mastodon posts containing '// # strudel' or strudel URLs.",

  "icons": {
    "48": "icons/spiral.png"
  },

  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["embed.js", "ready.js", "topfenstrudel.js"]
    }
  ],
  
  "permissions": [
    "https://strudel.tidalcycles.org/*"
  ]
}
